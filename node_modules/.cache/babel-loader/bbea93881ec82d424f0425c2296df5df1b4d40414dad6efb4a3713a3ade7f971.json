{"ast":null,"code":"// import { onMounted } from 'vue'\n// import CryptoJS from 'crypto-js';\nimport axios from 'axios';\nimport moment from 'moment';\nexport default {\n  name: 'app-pcBuilderAi',\n  data() {\n    return {\n      token: this.getToken,\n      tokenExpireTime: null,\n      lastRequest: moment().add(this.tokenExpireTime ?? 0, `seconds`)\n    };\n  },\n  methods: {\n    async getToken() {\n      if (moment().isAfter(this.lastRequest) || !this.tokenExpireTime) {\n        let response = await axios.post(`https://id.twitch.tv/oauth2/token?client_id=${process.env.VUE_APP_TWITCH_CLIENT_ID}&client_secret=${process.env.VUE_APP_TWITCH_CLIENT_SECRET}&grant_type=client_credentials`);\n        this.token = response.data.access_token;\n        this.tokenExpireTime = response.data.expires_in;\n        return response.data.access_token.toString();\n      }\n      console.log(this.tokenExpireTime);\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA;AACA;AACA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAO;AAEtB,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,IAAI,GAAG;IACH,OAAO;MACJC,KAAK,EAAE,IAAI,CAACC,QAAQ;MACpBC,eAAe,EAAE,IAAI;MACrBC,WAAW,EAAEN,MAAM,EAAE,CAACO,GAAG,CAAC,IAAI,CAACF,eAAc,IAAK,CAAC,EAAG,SAAQ;IACjE;EACJ,CAAC;EACDG,OAAO,EAAE;IACL,MAAMJ,QAAQ,GAAG;MACb,IAAGJ,MAAM,EAAE,CAACS,OAAO,CAAC,IAAI,CAACH,WAAW,KAAK,CAAC,IAAI,CAACD,eAAe,EAAC;QAC3D,IAAIK,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAE,+CAA8CC,OAAO,CAACC,GAAG,CAACC,wBAAyB,kBAAiBF,OAAO,CAACC,GAAG,CAACE,4BAA6B,gCAA+B;QAC7M,IAAI,CAACZ,KAAI,GAAIO,QAAQ,CAACR,IAAI,CAACc,YAAW;QACtC,IAAI,CAACX,eAAc,GAAIK,QAAQ,CAACR,IAAI,CAACe,UAAS;QAC9C,OAAOP,QAAQ,CAACR,IAAI,CAACc,YAAY,CAACE,QAAQ,EAAC;MAC/C;MACAC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,eAAe;IACpC;EACJ;AACJ","names":["axios","moment","name","data","token","getToken","tokenExpireTime","lastRequest","add","methods","isAfter","response","post","process","env","VUE_APP_TWITCH_CLIENT_ID","VUE_APP_TWITCH_CLIENT_SECRET","access_token","expires_in","toString","console","log"],"sourceRoot":"","sour