ait axios.post(`http://localhost:3000/${endpoint}`, query, {headers: {\"Content-Type\": 'text/plain'}})\r\n        console.log(response.data)\r\n      }catch(error) {\r\n        console.log('callEndPoint ERRO: ' + error)\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    gameSearched(nv) {\r\n\r\n        clearTimeout(this.timeout)\r\n\r\n        this.timeout = setTimeout(async () => {\r\n            let getGames = await this.searchGames('games', `search \"${nv}\"; fields name, screenshots; where platforms = 6;`)\r\n            let formatGames = getGames.data.map(async (game) => {\r\n                let getUrl = await this.searchGames('screenshots', `fields url; where id = ${game.screenshots[0]}`)\r\n                return {id: game.id, name: game.name, url: getUrl.data.url}\r\n            })\r\n            console.log(formatGames)\r\n        }, 2000)\r\n\r\n    },\r\n  },    \r\n  \r\n};\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}