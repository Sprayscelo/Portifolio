{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: \"gamesConfig\",\n  data() {\n    return {\n      gameSearched: \"\",\n      gamesFounded: ''\n    };\n  },\n  methods: {\n    async searchGames(endpoint, query) {\n      console.log(query);\n      //Always send the same header for this call, dont need to put on the parameter when calling.  \n      try {\n        const response = await axios.post(`http://localhost:3000/${endpoint}`, query, {\n          headers: {\n            \"Content-Type\": 'text/plain'\n          }\n        });\n        console.log(response.data);\n      } catch (error) {\n        console.log('callEndPoint ERRO: ' + error);\n      }\n    }\n  },\n  watch: {\n    gameSearched(nv) {\n      clearTimeout(this.timeout);\n      this.timeout = setTimeout(async () => {\n        let getGames = await this.searchGames('games', `search \"${nv}\"; fields name, screenshots; where platforms = 6;`);\n        let formatGames = getGames.data.map(async game => {\n          let getUrl = await this.searchGames('screenshots', `fields url; where id = ${game.screenshots[0]}`);\n          return {\n            id: game.id,\n            name: game.name,\n            url: getUrl.data.url\n          };\n        });\n        console.log(formatGames);\n      }, 2000);\n    }\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EAEbC,IAAI,EAAE,aAAa;EACnBC,IAAI,GAAG;IACL,OAAO;MACHC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAW,CAACC,QAAQ,EAAEC,KAAK,EAAE;MAC/BC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB;MACA,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAE,yBAAwBL,QAAS,EAAC,EAAEC,KAAK,EAAE;UAACK,OAAO,EAAE;YAAC,cAAc,EAAE;UAAY;QAAC,CAAC;QACvHJ,OAAO,CAACC,GAAG,CAACC,QAAQ,CAACT,IAAI;MAC3B,CAAC,QAAMY,KAAK,EAAE;QACZL,OAAO,CAACC,GAAG,CAAC,qBAAoB,GAAII,KAAK;MAC3C;IACF;EACF,CAAC;EACDC,KAAK,EAAE;IACLZ,YAAY,CAACa,EAAE,EAAE;MAEbC,YAAY,CAAC,IAAI,CAACC,OAAO;MAEzB,IAAI,CAACA,OAAM,GAAIC,UAAU,CAAC,YAAY;QAClC,IAAIC