n    border-bottom: 3px $outlineHeader solid;\n      ul {\n      display: flex;\n      gap: 15px;\n      list-style: none;\n      position: relative;\n      li {\n        cursor: pointer;\n        margin-right: 15px;\n        position: relative;\n        padding-bottom: 2px;\n        }\n        li::before, li:has(a.router-link-active) {\n          content: \"\";\n          background-color: white;\n          width: 0%;\n          height: 1.2px;\n          bottom: 0;\n          right: 50%;\n          position: absolute;\n          transform: translate(50%);\n        }\n        li:hover::before\n        {\n          text-align: center;\n          width: 100%;\n          transition: width .2s linear\n        }\n        a {\n          text-decoration: none;\n          color: $headerLetterColor;\n        }\n    }\n\n    \n\n  }\n\n  .redesSociais {\n    display: grid;\n    grid-template-columns: repeat(2, 1fr);\n    div{\n      margin-left: 10px;\n      width: 90%;\n      box-shadow: 0px 0px 5px;\n      border: 2px solid;\n      border-radius: 5px;\n      border-color: black;\n      background-color: rgb(220, 220, 220);\n      font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\n      a {\n        width: 60%;\n        text-decoration: none;\n        color:black;\n      }\n      padding: 5px 5px;\n      text-align: center;\n      img {\n        margin-right: 10px;\n        vertical-align: middle;\n        width: 30px;\n        height: 30px;\n      }\n    }\n  }\n\n  .funcoes {\n    color: $letras;\n    font-size: 20px;\n    text-decoration: underline;\n    text-decoration-skip: 5px;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}