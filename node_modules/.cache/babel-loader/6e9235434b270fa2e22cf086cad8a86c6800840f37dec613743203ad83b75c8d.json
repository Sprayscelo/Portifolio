/\r\n            },\r\n        },\r\n        watch: {\r\n            $route(to, from) {\r\n                if(to.path.includes('projetos/marvel')) {\r\n                    console.log('rodou aqui')\r\n                    this.apiMarvelRequest('/v1/public/characters', {\"offset\": '0'})\r\n                }\r\n            }\r\n        }\r\n        \r\n    }\r\n</script>\r\n\r\n<style>\r\n\r\ninput:hover {\r\n    background-color: black;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}