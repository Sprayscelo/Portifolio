{"ast":null,"code":"import CryptoJS from 'crypto-js';\nimport axios from 'axios';\nimport moment from 'moment';\nexport default {\n  name: 'app-marvel',\n  data() {\n    return {\n      marvelUrlBase: 'https://gateway.marvel.com'\n    };\n  },\n  methods: {\n    async apiMarvelRequest(endpoint, parameters) {\n      let hash = CryptoJS.MD5(2 + process.env.VUE_APP_MARVEL_PRIVATE_KEY + process.env.VUE_APP_MARVEL_PUBLIC_KEY).toString();\n      let urlBase = `${this.marvelUrlBase}${endpoint}?orderBy=name&apiKey=${process.env.VUE_APP_MARVEL_PUBLIC_KEY}&hash=${hash}&ts=2`;\n      for (let param in parameters) {\n        urlBase += `&${param}=${parameters[param]}`;\n      }\n      let response = await axios.get(urlBase);\n      console.log(response);\n      console.log(moment().unix());\n      //https://gateway.marvel.com/v1/public/characters?orderBy=name&apiKey=667baca984ac733ec50615060a6c3a07&hash=9c53c1fcffa0ca25c1cd3653de817926&ts=1\n    }\n  }\n};","map":{"version":3,"mappings":"AAOA,OAAOA,QAAO,MAAO,WAAW;AAChC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAO;AAEtB,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAG;IACH,OAAO;MACHC,aAAa,EAAE;IACnB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,gBAAgB,CAACC,QAAQ,EAAEC,UAAU,EAAE;MACzC,IAAIC,IAAG,GAAIV,QAAQ,CAACW,GAAG,CAAC,CAAC,GAACC,OAAO,CAACC,GAAG,CAACC,0BAA0B,GAACF,OAAO,CAACC,GAAG,CAACE,yBAAyB,CAAC,CAACC,QAAQ,EAAC;MACjH,IAAIC,OAAM,GAAK,GAAE,IAAI,CAACZ,aAAc,GAAEG,QAAS,wBAAuBI,OAAO,CAACC,GAAG,CAACE,yBAA0B,SAAQL,IAAK,OAAK;MAC9H,KAAI,IAAIQ,KAAI,IAAKT,UAAU,EAAE;QACzBQ,OAAM,IAAM,IAAGC,KAAM,IAAGT,UAAU,CAACS,KAAK,CAAE;MAC9C;MACA,IAAIC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAG,CAACH,OAAO;MACtCI,OAAO,CAACC,GAAG,CAACH,QAAQ;MACpBE,OAAO,CAACC,GAAG,CAACpB,MAAM,EAAE,CAACqB,IAAI,EAAE;MAC3B;IACJ;EACJ;AAEJ","names":["CryptoJS","axios","moment","name","data","marvelUrlBase","methods","apiMarvelRequest","endpoint","parameters","hash","MD5","process","env","VUE_APP_MARVEL_PRIVATE_KEY","VUE_APP_MARVEL_PUBLIC_KEY","toString","urlBase","param","response","get","console",