{"ast":null,"code":"// import { onMounted } from 'vue'\n// import CryptoJS from 'crypto-js';\nimport axios from 'axios';\nimport moment from 'moment';\nimport { onMounted } from 'vue';\nexport default {\n  name: 'app-pcBuilderAi',\n  data() {\n    return {\n      token: '',\n      tokenExpireTime: null,\n      lastRequest: null\n    };\n  },\n  methods: {\n    async getIgbToken() {\n      if (moment().isAfter(this.lastRequest) || !this.tokenExpireTime) {\n        let response = await axios.post(`https://id.twitch.tv/oauth2/token?client_id=${process.env.VUE_APP_TWITCH_CLIENT_ID}&client_secret=${process.env.VUE_APP_TWITCH_CLIENT_SECRET}&grant_type=client_credentials`);\n        this.token = response.data.access_token;\n        this.tokenExpireTime = response.data.expires_in;\n        this.lastRequest = moment().add(this.tokenExpireTime, 'seconds');\n        return response.data.access_token.toString();\n      }\n    },\n    async callEndPoint(url, method) {\n      if (method == `POST`) {\n        axios.post(url);\n      }\n    }\n  },\n  setup() {\n    onMounted(() => {\n      console.log('passou aqui');\n      this.getIgbToken();\n    });\n  }\n};","map":{"version":3,"mappings":"AAUA;AACA;AACA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,QAAO;AAC1B,SAASC,SAAQ,QAAS,KAAK;AAE3B,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,IAAI,GAAG;IACH,OAAO;MACJC,KAAK,EAAE,EAAE;MACTC,eAAe,EAAE,IAAI;MACrBC,WAAW,EAAE;IAChB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,WAAW,GAAG;MAChB,IAAGR,MAAM,EAAE,CAACS,OAAO,CAAC,IAAI,CAACH,WAAW,KAAK,CAAC,IAAI,CAACD,eAAe,EAAC;QAC3D,IAAIK,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAAE,+CAA8CC,OAAO,CAACC,GAAG,CAACC,wBAAyB,kBAAiBF,OAAO,CAACC,GAAG,CAACE,4BAA6B,gCAA+B;QAC7M,IAAI,CAACX,KAAI,GAAIM,QAAQ,CAACP,IAAI,CAACa,YAAW;QACtC,IAAI,CAACX,eAAc,GAAIK,QAAQ,CAACP,IAAI,CAACc,UAAS;QAC9C,IAAI,CAACX,WAAU,GAAIN,MAAM,EAAE,CAACkB,GAAG,CAAC,IAAI,CAACb,eAAe,EAAE,SAAS;QAC/D,OAAOK,QAAQ,CAACP,IAAI,CAACa,YAAY,CAACG,QAAQ,EAAC;MAC/C;IACJ,CAAC;IACD,MAAMC,YAAY,CAACC,GAAG,EAAEC,MAAM,EAAE;MAC5B,IAAGA,MAAK,IAA