{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "server.js"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/backend/$1" },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}
